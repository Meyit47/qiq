echo -e $b">>>"$w" qiq-mysystem   :"$g"sistemimi görüntüle"$w
echo -e $b">>>"$w" qiq-home       :"$g"dizine cd"$w
echo -e $b">>>"$w" qiq-usr        :"$g"dizine cd"$w
echo -e $b">>>"$w" qiq-bin        :"$g"dizine cd"$w
echo -e $b">>>"$w" qiq-dispatcher:"$g"Süreç araştırması"$w
