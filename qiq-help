echo -e $b">>>"$w" figlet -f Modular :"$g"açıklama yok"$w
echo -e $b">>>"$w" figlet -f Basic   :"$g"açıklama yok"$w
echo -e $b">>>"$w" figlet -f Rusto   :"$g"açıklama yok"$w
