g="\033[1;32m"
r="\033[1;31m"
b="\033[1;34m"
w="\033[0m"

echo -e $b">>>"$w" qiq bilgi: "$g"help"$w
echo ""
echo -e $b">>>"$w" qiq-help       :"$g"Bütün qiq bilgisi görüntüle"$w
echo -e $b">>>"$w" qiq-update     :"$g"qiq güncelle"$w
echo -e $b">>>"$w" qiq-version    :"$g"qiq sürümünü görüntüle"$w
echo -e $b">>>"$w" qiq-author     :"$g"qiq Sahibini görüntüle"$w
